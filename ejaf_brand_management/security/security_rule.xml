<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="product_brand_rule_assign_users" model="ir.rule">
            <field name="name">Product Brand Assign Teams</field>
            <field ref="product.model_product_template" name="model_id"/>
            <field name="domain_force">['|','|', ('brand_id', '=', False),('brand_id.team_ids','=',False),('brand_id.team_users_ids','in',user.id)]</field>
            <field name="groups" eval="[(4, ref('sales_team.group_sale_salesman'))]"/>
        </record>
        <record id="product_brand_rule_sales_manager" model="ir.rule">
            <field name="name">Product Brand Sale Manager</field>
            <field ref="product.model_product_template" name="model_id"/>
            <field name="domain_force">[(1, '=', 1)]</field>
            <field name="groups" eval="[(4, ref('sales_team.group_sale_manager'))]"/>
        </record>


    </data>
</odoo>