<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="see_subscription_menu" model="res.groups">
            <field name="name">See Subscription Menu</field>
            <field name="implied_ids" eval="[(4, ref('sale_subscription.group_sale_subscription_view'))]"/>
            <field name="category_id" ref="base.module_category_sales_subscription"/>

        </record>

        <record id="sale_subscription.group_sale_subscription_manager" model="res.groups">
            <field name="implied_ids" eval="[(4, ref('ejaf_sales_groups.see_subscription_menu'))]"/>
        </record>

        <record id="sale_subscription.menu_sale_subscription_root" model="ir.ui.menu">
            <field name="groups_id"
                   eval="[(4, ref('ejaf_sales_groups.see_subscription_menu')), (3, ref('sale_subscription.group_sale_subscription_view'))]"/>
        </record>

    </data>
</odoo>
