<odoo>
    <data>

        <record id="pos_user_rule" model="ir.rule">
            <field name="name">User can only see pos related to its sales team only</field>
            <field name="model_id" ref="point_of_sale.model_pos_config"/>
            <field name="domain_force">['|', ('crm_team_id.member_ids', 'in', user.id), ('crm_team_id', 'in', user.sales_team_ids.ids)]</field>
        </record>
    </data>
</odoo>